public class trycatch
{
    public stativ void main(string[] args)
    {
        try
        {
            int arr[]={1,3,5,7}
            System.out,println(arr[10]);
        }
        catch(ArrayIndexOutOfBoundsException e)
        {
            System.out.println(e);
        }
        System.out.println("test the  code:");
    }
}
