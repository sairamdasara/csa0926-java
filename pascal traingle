class RightAngleTriangle {
    public static void main(String[] args) {
        int rows = 5; 
        for (int i = 1; i <=rows; i++) {
            for (int j = 0; j < rows - i; j++) {
                System.out.print(" ");
            }
            int c=1;
            for (int k = 1; k <= i; k++) 
            {
                System.out.print(c+" ");
                c=c*(i-k)/k;
            }
            System.out.println();
        }
    }
}
