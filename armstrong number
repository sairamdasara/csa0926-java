import java.io.*;
import java.util.*;
 public class armstrong
{
    public static void main(String[] args)
    {
        int r,sum=0,temp;
        Scanner sc= new Scanner(System.in);
        System.out.println("enter a number");
        int num=sc.nextInt();
        {
        temp=num;
        while(num>0)
        {
            r=num%10;
            sum=sum+(r*r*r);
            num=num/10;
        }
            if(temp==sum)
                System.out.println("armstrong number");
            else
                System.out.println("not a armstrong number");
        }
        
    }
}
