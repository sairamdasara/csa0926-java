import java.io.*;
import java.util.*;
 public class perfect
{
    public static void main(String[] args)
    {
        int i,sum=0;
        Scanner sc= new Scanner(System.in);
        System.out.println("enter a number");
        int num=sc.nextInt();
        for(i=1;i<num;i++)
        {
            if(num %i==0)
            sum=sum+i;
        }
        if(sum==num)
        System.out.println("perfect number");
        else
        System.out.println("not perfect number");
    }
}
        
